{
  "name": "react-ts-strict-eslint-config",
  "version": "1.0.0",
  "description": "Very strict ESLint config for React projects using TypeScript",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./whitespace": "./whitespace.js",
    "./rules/best-practices": "./rules/best-practices.js",
    "./rules/es6": "./rules/es6.js",
    "./rules/node": "./rules/node.js",
    "./rules/style": "./rules/style.js",
    "./rules/errors": "./rules/errors.js",
    "./rules/imports": "./rules/imports.js",
    "./rules/strict": "./rules/strict.js",
    "./rules/variables": "./rules/variables.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "prelint": "eclint check * rules/* test/*",
    "lint": "eslint --report-unused-disable-directives .",
    "prepublish": "eslint-find-rules --unused"
  },
  "keywords": [
    "typescript",
    "eslint",
    "eslintconfig",
    "config",
    "typescript",
    "es2018",
    "react"
  ],
  "author": "David Recheni <http://davidrecheni.com>",
  "license": "MIT",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-tape-runner": "^3.0.0",
    "doctoc": "2.2.1",
    "eclint": "^2.8.1",
    "eslint": "^8.32.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "in-publish": "^2.0.1",
    "safe-publish-latest": "^2.0.0",
    "tape": "^5.6.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.11",
    "object.assign": "^4.1.4",
    "object.entries": "^1.1.6",
    "semver": "^7.3.8"
  },
  "packageManager": "yarn@3.5.0"
}
