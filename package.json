{
  "name": "eslint-config-ts-strict",
  "version": "1.3.9",
  "description": "Very strict ESLint config for projects using TypeScript",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./whitespace": "./whitespace.js",
    "./rules/best-practices": "./rules/best-practices.js",
    "./rules/es6": "./rules/es6.js",
    "./rules/node": "./rules/node.js",
    "./rules/style": "./rules/style.js",
    "./rules/errors": "./rules/errors.js",
    "./rules/imports": "./rules/imports.js",
    "./rules/strict": "./rules/strict.js",
    "./rules/stylistic": "./rules/stylistic.js",
    "./rules/variables": "./rules/variables.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "prelint": "eclint check * rules/* test/*",
    "lint": "eslint --report-unused-disable-directives .",
    "prepublish": "eslint-find-rules --unused"
  },
  "keywords": [
    "typescript",
    "eslint",
    "eslintconfig",
    "config",
    "typescript",
    "es2018",
    "react"
  ],
  "author": "David Recheni <http://davidrecheni.com>",
  "license": "MIT",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^3.2.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.24.4",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "babel-tape-runner": "^3.0.0",
    "doctoc": "2.2.1",
    "eclint": "^2.8.1",
    "eslint": "^8.57.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "in-publish": "^2.0.1",
    "safe-publish-latest": "^2.0.0",
    "tape": "^5.7.5",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.11",
    "object.assign": "^4.1.5",
    "object.entries": "^1.1.8",
    "semver": "^7.6.0"
  }
}
